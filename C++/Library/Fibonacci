// フィボナッチ数列
int Fibonacci (int N) {
    if (N == 0) return 0;
    else if (N == 1) return 1;
    
    return Fibonacci(N - 1) + Fibonacci(N - 2);
}

// トリボナッチ数列
int Tribonacci (int N) {
    if (N == 0) return 0;
    else if (N == 1) return 0;
    else if (N == 2) return 1;

    return Tribonacci(N - 1) + Tribonacci(N - 2) + Tribonacci(N - 3);
}
